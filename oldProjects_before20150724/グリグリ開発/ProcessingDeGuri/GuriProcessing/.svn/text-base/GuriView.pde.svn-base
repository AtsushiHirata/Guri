/**
 * GUIの図形表示部分に、現在取り扱っているデータを描き出す役を担う
 */
class GuriView{
  
  GuriData gd; // これから描くグリグリデータへの参照
  
  /**
   * Constructor　引数無し
   */
   public GuriView(){
     gd = new GuriData();
   }
   
  /**
   * Constructor　引数有り
   */
   public GuriView(GuriData newgd){
     gd = newgd;
   }
   
   public GuriData getDataObject(){
